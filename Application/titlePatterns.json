{
    "sep": "[ .\\-_:]",
    "sectionSeparator":[
        "[\\(\\)\\[\\]\\{\\}/]",
        "[\\-=:!]{2,}",
        "^",
        "$"
    ],
    "anySep":"(%sep|%sectionSeparator)",
    "year":"%anySep(?<year>(19|20|21)[0-9]{2})%anySep",
    "seasonKeyword":"(s|season|saison)",
    "episodeKeyword":"(e|ep|episode)",
    "romanSeasonNumber":"(I|II|III|IV|V|VI|VII|VIII|IX|X|XI|XII|XIII)",
    "seasonNumber":"(0?[1-9]|[1-3][0-9])",
    "episodeNumber":"(0{0,2}[1-9]|0?[1-9][0-9]|[1-3][0-9]{2})",
    "seasonID": [
        "%anySep%seasonKeyword%sep*(?<seasonNumber6>%seasonNumber)%anySep",
        "%anySeplivre%sep+(?<seasonNumber7>%romanSeasonNumber)%anySep"
    ],
    "episodeID":"%anySep%episodeKeyword%sep*(?<episodeNumber6>%episodeNumber)%anySep",
    "completeEpisodeID":[
        "%seasonKeyword%sep*(?<seasonNumber1>%seasonNumber)%sep*%episodeKeyword%sep*(?<episodeNumber1>%episodeNumber)($|[^0-9])",
        "%anySep(?<seasonNumber2>%seasonNumber)x(?<episodeNumber2>%episodeNumber)%anySep",
        "%anySep(?<seasonNumber3>0[1-9])(?<episodeNumber3>0[1-9]|[1-2][0-9])%anySep"
    ],
    "maybeCompleteEpisodeID":[
        "%anySep(?<seasonNumber4>[1-9])(?<episodeNumber4>0[1-9]|[1-2][0-9])%anySep",
        "%anySep(?<seasonNumber5>%seasonNumber)\\.(?<episodeNumber5>%episodeNumber)%anySep"
    ],
    "wordFollowedByName":[
        "de",
        "with",
        "avec",
        "by",
        "con"
    ],
    "nonWord":[
        "%completeEpisodeID",
        "%seasonID",
        "webisode",
        "blu-?ray",
        "1080p",
        "720p",
        "(1280|1920|2560|3840)x[0-9]{3,4}",
        "[0-9]{3,4}x(720|1080|2160)",
        "[hx]264",
        "divx",
        "xvid",
        "truefrench",
        "cine-pro",
        "hd%sep{0,1}dvd",
        "hd%sep{0,1}tv",
        "%sepAAC(%sep|$)",
        "%sepAC3(%sep|$)",
        "%sepDTS(%sep|$)",
        "%sepMP3(%sep|$)",
        "%sepaka%sep.",
        "(br|bd|dvd|web)%sep{0,1}rip",
        "%sephd(%sep|$)",
        "%sep{2}(%wordFollowedByName|film|movie)",
        "%sep[1-9]%sep+(de|of)%sep+[2-9](%sep|$)",
        "%sepcd[1-9](%sep|$)",
        "%sepvost(|[a-z]{2,3})%anySep"
    ],

    "likelyEndingWordNotPartOfTitle":[
        "FRENCH",
        "LIMITED",
        "REMASTERED",
        "EXTENDED",
        "MULTI"
    ],
    "subSectionSeparator":"%sep{3}",
    "directorsAndActorsNames":{"listFromFile":"directorsAndActorsNames.txt"},
    "languageID":[
        "fr",
        "en",
        "it",
        "ru",
        "ch"
    ],
    "likelyNonTitle":[
        "%sep[a-z]\\.%sep*[a-z]",
        "%directorsAndActorsNames",
        "%anySepBBC%anySep",
        "%anySepHBO%anySep",
        "%anySep%languageID%anySep"
    ],
    "startingSeparators":"^%sep*",
    "endingSeparators":"%sep*$",
    "castPreview":"%sep%wordFollowedByName%sep*%directorsAndActorsNames",
    "counter":"(^|%sep)(?<counterValue>[0-9]{2,3})(%sep|:|$)",
    "lowercaseChar":"[a-z]",
    "endingUppercaseWord":"%sep[A-Z]{3,}$",
    "wordChar":"[[:word:]]",


    "patternsThatAreCaseSensitive":[
        "lowercaseChar",
        "endingUppercaseWord"
    ]
}
